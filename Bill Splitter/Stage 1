print("Enter the number of friends joining (including you):")
n = int(input())
print()

if n<=0:
    print("No one is joining for the party")
else:
    print("Enter the name of every friend (including you), each on a new line:")
    s = {}
    for i in range(n):
        name = input()
        s[name] = 0
    
    print()
    print(s)
